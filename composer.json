{
	"name" : "elasticms/core-bundle",
	"description" : "The elasticms core bundle",
	"type" : "symfony-bundle",
	"authors" : [{
			"name" : "Mathieu De Keyzer",
			"email" : "mathieu@dekeyzer.net"
		},
		{
			"name" : "David Meert",
			"email" : "david.meert@smals.be"
		}
	],
	"keywords" : [
		"elasticms"
	],
	"license" : "MIT",
	"require" : {
		"php": "^7.4",
		"ext-iconv": "*",
		"ext-json": "*",
		"ext-openssl": "*",
		"ext-soap": "*",
		"ext-zip": "*",
		"ext-dom": "*",
		"ext-libxml": "*",
		"caxy/php-htmldiff": "^0.1",
		"doctrine/doctrine-migrations-bundle" : "^1.3",
		"elasticms/client-helper-bundle": "^3.6",
		"guzzlehttp/guzzle" : "^6.3",
		"psr/cache": "^1.0",
		"psr/link": "~1.0.0",
		"psr/simple-cache": "^1.0",
		"sensio/framework-extra-bundle": "^5.4",
		"sensiolabs/ansi-to-html" : "^1.1",
		"symfony/asset": "^4.4",
		"symfony/console": "^4.4",
		"symfony/expression-language": "^4.4",
		"symfony/form": "^4.4",
		"symfony/monolog-bridge": "^4.4",
		"symfony/cache": "^4.4",
		"doctrine/persistence": "^1.3",
		"doctrine/orm": "^2.7",
		"doctrine/doctrine-bundle": "^1.12",
		"symfony/process": "^4.4",
		"symfony/security": "^4.4",
		"symfony/security-bundle": "^2.8 || ^3.0 || ^4.0",
		"symfony/serializer-pack": "^1.0",
		"symfony/swiftmailer-bundle": "^3.4",
		"symfony/twig-bundle": "^4.4",
		"symfony/validator": "^4.4",
		"symfony/web-link": "^4.4",
		"symfony/webpack-encore-pack": "^1.0",
		"symfony/yaml": "^4.4",
		"symfony/ldap": "^4.4",
		"ramsey/uuid-doctrine": "^1.6",
		"rector/rector": "^0.8.56"
	},
	"require-dev" : {
		"friendsofphp/php-cs-fixer" : "^2.16",
		"phpstan/phpstan": "^0.12",
		"phpunit/phpunit" : "^7.1",
		"symfony/test-pack": "^1.0",
		"squizlabs/php_codesniffer": "3.4"
	},
	"autoload" : {
		"psr-4" : {
			"EMS\\CoreBundle\\" : "src/",
			"FOS\\UserBundle\\" : "src-user/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"EMS\\CoreBundle\\Tests\\" : "tests/"
		}
	},
	"config": {
		"lock": false,
		"sort-packages": true
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.8.x-dev",
			"dev-demo" : "1.9.x-dev"
		}
	},
	"scripts": {
		"phpcs": "php-cs-fixer fix",
		"phpstan": "phpstan analyse",
		"phpunit": "phpunit"
	}
}
