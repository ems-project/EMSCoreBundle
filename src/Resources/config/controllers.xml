<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults autowire="true" public="true"/>

        <!-- Api\Admin -->
        <service id="EMS\CoreBundle\Controller\Api\Admin\EntitiesController">
            <argument type="service" id="emsco.helper.entities" />
            <argument type="service" id="emsco.logger" />
            <tag name="controller.service_arguments"/>
        </service>
        <service id="EMS\CoreBundle\Controller\Api\Admin\MetaController">
            <argument type="service" id="ems.service.contenttype" />
            <tag name="controller.service_arguments"/>
        </service>

        <!-- Api\File -->
        <service id="EMS\CoreBundle\Controller\Api\File\ExtractDataController">
            <argument type="service" id="ems.service.asset_extractor" />
            <tag name="controller.service_arguments"/>
        </service>

        <!-- Api\Form -->
        <service id="EMS\CoreBundle\Controller\Api\Form\SubmissionController">
            <argument type="service" id="ems.form_submission" />
            <argument type="service" id="logger" />
            <tag name="controller.service_arguments"/>
        </service>
        <service id="EMS\CoreBundle\Controller\Api\Form\VerificationController">
            <argument type="service" id="ems.form_verification" />
            <argument type="service" id="logger" />
            <tag name="controller.service_arguments"/>
        </service>

        <!-- Api\Search -->
        <service id="EMS\CoreBundle\Controller\Api\Search\SearchController">
            <argument type="service" id="ems_common.service.elastica"/>
            <tag name="controller.service_arguments"/>
        </service>

        <!-- Api -->
        <service id="EMS\CoreBundle\Controller\Api\AuthTokenLoginController"/>

        <!-- ContentManagement -->
        <service id="EMS\CoreBundle\Controller\ContentManagement\ActionController">
            <argument type="service" id="logger" />
            <argument type="service" id="ems.service.action" />
            <tag name="controller.service_arguments"/>
        </service>
        <service id="EMS\CoreBundle\Controller\ContentManagement\AnalyzerController">
            <argument type="service" id="emsco.logger" />
            <argument type="service" id="ems.service.helper" />
        </service>
        <service id="EMS\CoreBundle\Controller\ContentManagement\AssetController">
            <argument type="service" id="ems_common.storage.processor" />
            <argument type="service" id="ems.repository.channel" />
            <argument>%ems_core.asset_config%</argument>
            <tag name="controller.service_arguments"/>
        </service>
        <service id="EMS\CoreBundle\Controller\ContentManagement\ContentTypeController">
            <argument type="service" id="emsco.logger" />
            <argument type="service" id="ems.service.contenttype" />
            <argument type="service" id="ems.service.mapping" />
            <argument type="service" id="form.registry" />
        </service>
        <service id="EMS\CoreBundle\Controller\ContentManagement\CrudController">
            <argument type="service" id="emsco.logger" />
            <argument type="service" id="ems.service.user" />
            <argument type="service" id="ems.service.data" />
            <argument type="service" id="ems.service.contenttype" />
        </service>
        <service id="EMS\CoreBundle\Controller\ContentManagement\DataController">
            <argument type="service" id="logger" />
            <argument type="service" id="ems.service.data" />
            <argument type="service" id="ems.service.search" />
            <argument type="service" id="ems.service.contenttype" />
            <argument type="service" id="ems.service.environment" />
            <argument type="service" id="ems.service.index" />
            <argument type="service" id="translator" />
            <argument type="service" id="ems.content_type.view_types" />
            <argument type="service" id="twig" />
            <argument type="service" id="ems_common.pdf.printer.dom" />
            <argument type="service" id="ems.service.job" />
            <tag name="controller.service_arguments"/>
        </service>
        <service id="EMS\CoreBundle\Controller\ContentManagement\DatatableController">
            <argument type="service" id="ems.service.datatable" />
            <argument type="service" id="ems_core.core_data_table.table_renderer" />
            <argument type="service" id="ems_core.core_data_table.table_exporter" />
            <tag name="controller.service_arguments"/>
        </service>
        <service id="EMS\CoreBundle\Controller\ContentManagement\EnvironmentController">
            <argument type="service" id="emsco.logger" />
            <argument type="service" id="ems.service.search" />
            <argument type="service" id="ems.service.environment" />
            <argument type="service" id="ems.service.contenttype" />
            <argument type="service" id="security.authorization_checker" />
            <argument type="service" id="ems.service.publish" />
            <argument type="service" id="ems.service.index" />
            <argument type="service" id="ems.service.mapping" />
            <argument type="service" id="ems.service.alias" />
            <argument type="service" id="ems.service.job" />
            <argument>%ems_core.paging_size%</argument>
            <argument>%ems_core.instance_id%</argument>
            <argument>%ems_core.circles_object%</argument>
        </service>
        <service id="EMS\CoreBundle\Controller\ContentManagement\FileController">
            <argument type="service" id="ems.service.file" />
            <argument type="service" id="ems.service.asset_extractor" />
            <argument type="service" id="emsco.logger" />
        </service>
        <service id="EMS\CoreBundle\Controller\ContentManagement\FilterController">
            <argument type="service" id="emsco.logger" />
            <argument type="service" id="ems.service.helper" />
        </service>
        <service id="EMS\CoreBundle\Controller\ContentManagement\IndexController">
            <argument type="service" id="ems.service.index" />
        </service>
        <service id="EMS\CoreBundle\Controller\ContentManagement\JobController">
            <argument type="service" id="emsco.logger" />
            <argument type="service" id="ems.service.job" />
            <argument>%ems_core.paging_size%</argument>
            <argument>%ems_core.trigger_job_from_web%</argument>
            <tag name="controller.service_arguments"/>
        </service>
        <service id="EMS\CoreBundle\Controller\ContentManagement\ManagedAliasController">
            <argument type="service" id="emsco.logger" />
            <argument type="service" id="ems.service.alias" />
            <argument>%ems_core.instance_id%</argument>
        </service>
        <service id="EMS\CoreBundle\Controller\ContentManagement\PublishController">
            <argument type="service" id="ems.service.publish" />
            <argument type="service" id="ems.service.job" />
            <argument type="service" id="ems.service.environment" />
            <argument type="service" id="ems.service.contenttype" />
            <argument type="service" id="ems.service.search" />
            <argument type="service" id="ems_common.service.elastica" />
        </service>
        <service id="EMS\CoreBundle\Controller\ContentManagement\ReleaseController">
            <argument type="service" id="logger" />
            <argument type="service" id="ems.service.release" />
            <argument type="service" id="ems.service.release_revision" />
            <tag name="controller.service_arguments"/>
        </service>
        <service id="EMS\CoreBundle\Controller\ContentManagement\ViewController">
            <argument type="service" id="ems.service.contenttype" />
            <argument type="service" id="ems.view.manager" />
            <argument type="service" id="logger"/>
            <tag name="controller.service_arguments"/>
        </service>

        <!-- Dashboard -->
        <service id="EMS\CoreBundle\Controller\Dashboard\DashboardController">
            <argument type="service" id="logger"/>
            <argument type="service" id="ems.dashboard.manager" />
            <tag name="controller.service_arguments"/>
        </service>

        <!-- Form -->
        <service id="EMS\CoreBundle\Controller\Form\SubmissionController">
            <argument type="service" id="ems.form_submission" />
            <argument type="service" id="logger" />
            <tag name="controller.service_arguments"/>
        </service>

        <!-- Job -->
        <service id="EMS\CoreBundle\Controller\Job\ScheduleController">
            <argument type="service" id="ems.schedule.manager" />
            <argument type="service" id="logger"/>
            <tag name="controller.service_arguments"/>
        </service>

        <!-- Log -->
        <service id="EMS\CoreBundle\Controller\Log\LogController">
            <argument type="service" id="ems.log.manager" />
            <argument type="service" id="logger" />
            <tag name="controller.service_arguments"/>
        </service>

        <!-- Revision -->
        <service id="EMS\CoreBundle\Controller\Revision\DetailController">
            <argument type="service" id="ems.service.contenttype" />
            <argument type="service" id="EMS\CoreBundle\Repository\EnvironmentRepository" />
            <argument type="service" id="ems.service.data" />
            <argument type="service" id="ems.service.revision" />
            <argument type="service" id="EMS\CoreBundle\Repository\RevisionRepository" />
            <argument type="service" id="ems_common.service.elastica" />
            <argument type="service" id="ems.service.search" />
            <argument type="service" id="ems.log.manager"/>
            <argument type="service" id="logger" />
            <tag name="controller.service_arguments"/>
        </service>
        <service id="EMS\CoreBundle\Controller\Revision\EditController">
            <argument type="service" id="ems.service.data" />
            <argument type="service" id="ems_core.core_revision.draft_in_progress" />
            <argument type="service" id="logger" />
            <argument type="service" id="ems.service.publish" />
            <argument type="service" id="ems.service.revision" />
            <argument type="service" id="translator" />
            <tag name="controller.service_arguments"/>
        </service>
        <service id="EMS\CoreBundle\Controller\Revision\JsonMenuNestedController">
            <argument type="service" id="ems_core.core_ui.ajax_service" />
            <argument type="service" id="EMS\CoreBundle\Core\Revision\Json\JsonMenuRenderer" />
            <argument type="service" id="ems.service.revision" />
            <argument type="service" id="ems.service.data" />
            <argument type="service" id="ems.service.user" />
            <tag name="controller.service_arguments"/>
        </service>
        <service id="EMS\CoreBundle\Controller\Revision\TaskController">
            <argument type="service" id="ems_core.core_revision_task.task_manager" />
            <argument type="service" id="ems_core.core_ui.ajax_service" />
            <argument type="service" id="form.factory" />
            <argument type="service" id="ems_core.core_data_table.table_exporter" />
            <tag name="controller.service_arguments"/>
        </service>

        <!-- Search -->
        <service id="EMS\CoreBundle\Controller\Search\QuerySearchController">
            <argument type="service" id="ems.service.query_search" />
            <argument type="service" id="EMS\CoreBundle\Controller\ElasticsearchController" />
            <argument type="service" id="ems_core.core_document.data_links_factory" />
            <tag name="controller.service_arguments"/>
        </service>

        <!-- User -->
        <service id="EMS\CoreBundle\Controller\User\LoginController">
            <argument type="service" id="twig"/>
            <tag name="controller.service_arguments"/>
        </service>
        <service id="EMS\CoreBundle\Controller\User\ProfileController">
            <argument type="service" id="emsco.manager.user" />
            <argument type="service" id="emsco.logger" />
        </service>
        <service id="EMS\CoreBundle\Controller\User\ResettingController">
            <argument type="service" id="emsco.manager.user" />
            <argument type="service" id="ems_core.security.authenticator" />
            <argument type="service" id="emsco.logger" />
        </service>

        <!-- Views -->
        <service id="EMS\CoreBundle\Controller\Views\CalendarController">
            <argument type="service" id="emsco.logger" />
            <argument type="service" id="ems_common.service.elastica" />
            <argument type="service" id="ems.service.data" />
            <argument type="service" id="ems.service.search" />
        </service>
        <service id="EMS\CoreBundle\Controller\Views\CriteriaController">
            <argument type="service" id="emsco.logger" />
            <argument type="service" id="ems_common.service.elastica" />
            <argument type="service" id="ems.service.data" />
            <argument type="service" id="ems.service.contenttype" />
            <argument type="service" id="ems.form.factories.objectChoiceListFactory" />
            <argument type="service" id="security.authorization_checker" />
            <argument type="service" id="form.registry" />
        </service>
        <service id="EMS\CoreBundle\Controller\Views\HierarchicalController">
            <argument type="service" id="ems.service.contenttype" />
            <argument type="service" id="ems.service.search" />
        </service>

        <!-- Wysiwyg -->
        <service id="EMS\CoreBundle\Controller\Wysiwyg\StylesetController">
            <argument type="service" id="ems.service.wysiwyg_styles_set" />
            <tag name="controller.service_arguments"/>
        </service>

        <!-- Controllers -->
        <service id="EMS\CoreBundle\Controller\ChannelController">
            <argument type="service" id="logger" />
            <argument type="service" id="ems.service.channel" />
            <tag name="controller.service_arguments"/>
        </service>
        <service id="EMS\CoreBundle\Controller\DashboardController">
            <argument type="service" id="ems.dashboard.manager" />
            <argument type="service" id="ems_core.dashboard.dashboards" />
            <tag name="controller.service_arguments"/>
        </service>
        <service id="EMS\CoreBundle\Controller\ElasticsearchController">
            <argument type="service" id="logger" />
            <argument type="service" id="ems.service.index" />
            <argument type="service" id="ems_common.service.elastica"/>
            <argument type="service" id="ems.service.data" />
            <argument type="service" id="ems.service.asset_extractor" />
            <argument type="service" id="ems.service.environment" />
            <argument type="service" id="ems.service.contenttype" />
            <argument type="service" id="ems.service.search" />
            <argument type="service" id="security.authorization_checker" />
            <argument type="service" id="ems.service.job" />
            <argument type="service" id="ems.service.aggregate_option" />
            <argument type="service" id="ems.service.sort_option" />
            <argument type="service" id="ems.dashboard.manager" />
            <argument>%ems_core.paging_size%</argument>
            <argument>%ems_core.health_check_allow_origin%</argument>
            <argument>%ems_core.elasticsearch_cluster%</argument>
            <tag name="controller.service_arguments"/>
        </service>
        <service id="EMS\CoreBundle\Controller\I18nController">
            <argument type="service" id="ems.service.i18n" />
            <argument>%ems_core.paging_size%</argument>
        </service>
        <service id="EMS\CoreBundle\Controller\NotificationController">
            <argument type="service" id="logger"/>
            <argument type="service" id="ems.service.publish"/>
            <argument type="service" id="ems.service.environment"/>
            <argument type="service" id="doctrine"/>
            <argument type="service" id="ems.service.notification"/>
            <argument type="service" id="ems.dashboard.manager"/>
            <argument>%ems_core.paging_size%</argument>
            <tag name="controller.service_arguments"/>
        </service>
        <service id="EMS\CoreBundle\Controller\QuerySearchController">
            <argument type="service" id="logger" />
            <argument type="service" id="ems.service.query_search" />
            <tag name="controller.service_arguments"/>
        </service>
        <service id="EMS\CoreBundle\Controller\SearchController">
            <argument type="service" id="ems.service.sort_option" />
            <argument type="service" id="ems.service.aggregate_option" />
            <argument type="service" id="ems.service.search_field_option" />
            <argument type="service" id="translator" />
        </service>
        <service id="EMS\CoreBundle\Controller\TwigElementsController">
            <argument type="service" id="ems.service.asset_extractor"/>
            <argument type="service" id="ems_common.service.elastica"/>
            <argument type="service" id="ems.service.user"/>
            <argument type="service" id="ems.service.job"/>
            <argument type="service" id="ems.dashboard.manager"/>
            <argument type="service" id="ems.service.contenttype"/>
            <tag name="controller.service_arguments"/>
        </service>
        <service id="EMS\CoreBundle\Controller\UploadedFileController">
            <argument type="service" id="emsco.logger" />
            <argument type="service" id="ems.service.file" />
        </service>
        <service id="EMS\CoreBundle\Controller\UploadedFileWysiwygController">
            <argument type="service" id="ems.service.file" />
            <argument type="service" id="ems_core.core_ui.ajax_service" />
            <tag name="controller.service_arguments"/>
        </service>
        <service id="EMS\CoreBundle\Controller\UserController">
            <argument type="service" id="logger" />
            <argument>%ems_core.circles_object%</argument>
            <argument type="service" id="ems.service.user" />
            <argument type="service" id="emsco.manager.user" />
            <argument type="service" id="EMS\CommonBundle\Contracts\SpreadsheetGeneratorServiceInterface"/>
            <tag name="controller.service_arguments"/>
        </service>
        <service id="EMS\CoreBundle\Controller\WysiwygController">
            <argument type="service" id="ems.service.wysiwyg_profile" />
            <argument type="service" id="ems.service.wysiwyg_styles_set" />
            <argument type="service" id="translator" />
        </service>

    </services>
</container>
