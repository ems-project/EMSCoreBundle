<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing https://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="emsco_analyzer_index" path="/"
           controller="EMS\CoreBundle\Controller\ContentManagement\AnalyzerController::index"
           methods="GET|POST"/>
    <route id="emsco_analyzer_edit" path="/edit/{analyzer}"
           controller="EMS\CoreBundle\Controller\ContentManagement\AnalyzerController::edit"
           methods="GET|POST"/>
    <route id="emsco_analyzer_delete" path="/delete/{analyzer}"
           controller="EMS\CoreBundle\Controller\ContentManagement\AnalyzerController::delete"
           methods="POST"/>
    <route id="emsco_analyzer_add" path="/add"
           controller="EMS\CoreBundle\Controller\ContentManagement\AnalyzerController::add"
           methods="GET|POST"/>
    <route id="emsco_analyzer_export" path="/export/{analyzer}.json"
           controller="EMS\CoreBundle\Controller\ContentManagement\AnalyzerController::export"
           methods="GET"/>
</routes>
