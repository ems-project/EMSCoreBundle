<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing https://symfony.com/schema/routing/routing-1.0.xsd">

  <route id="emsco_form_admin_index" path="/admin/form"
         controller="EMS\CoreBundle\Controller\Form\FormController::index"
         methods="GET|POST"/>
  <route id="emsco_form_admin_add" path="/admin/form/add"
         controller="EMS\CoreBundle\Controller\Form\FormController::add"
         methods="GET|POST"/>
  <route id="emsco_form_admin_edit" path="/admin/form/edit/{form}"
         controller="EMS\CoreBundle\Controller\Form\FormController::edit"
         methods="GET|POST"/>
  <route id="emsco_form_admin_delete" path="/admin/form/delete/{form}"
         controller="EMS\CoreBundle\Controller\Form\FormController::delete"
         methods="POST"/>
  <route id="emsco_form_admin_reorder" path="/admin/form/reorder/{form}"
         controller="EMS\CoreBundle\Controller\Form\FormController::reorder"
         methods="GET|POST"/>
</routes>
