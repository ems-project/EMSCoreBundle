<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing https://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="emsco_uploaded_asset_publisher_overview" path="/"
           controller="EMS\CoreBundle\Controller\UploadedFileController::publisherIndex"
           methods="GET|POST"/>
    <route id="emsco_uploaded_asset_publisher_hide" path="/delete/{hash}"
           controller="EMS\CoreBundle\Controller\UploadedFileController::publisherHideByHash"
           methods="POST"/>

</routes>
