<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing https://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="ems_core_uploaded_file_logs" path="/"
           controller="EMS\CoreBundle\Controller\UploadedFileController::logs"
           methods="GET|POST"/>
    <route id="ems_core_uploaded_file_show_hide" path="/show-hide/{assetId}"
           controller="EMS\CoreBundle\Controller\UploadedFileController::showHide"
           methods="POST"/>

    <route id="ems_core_uploaded_file_logs_ajax" path="/datatable.json"
           controller="EMS\CoreBundle\Controller\UploadedFileController::ajaxDataTable"
           methods="GET"/>
</routes>
