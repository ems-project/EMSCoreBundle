<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing https://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="emsco_user_login" path="/login"
           controller="EMS\CoreBundle\Controller\User\LoginController::login"
           methods="GET|POST"/>
    <route id="emsco_user_logout" path="/logout"
           controller="EMS\CoreBundle\Controller\User\LoginController::logout"
           methods="GET|POST"/>

</routes>