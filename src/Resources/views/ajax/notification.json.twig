{
   "acknowledged": true,
   	{% set notice = app.request.hasSession(true) ? app.session.flashbag.get('notice') : false %}
	{% if notice %}
		"notice": {{ notice|json_encode|raw }},
	{% endif %}
	{% set warning = app.request.hasSession(true) ? app.session.flashbag.get('warning') : false  %}
	{% if warning %}
		"warning": {{ warning|json_encode|raw }},
	{% endif %}
	{% set error = app.request.hasSession(true) ? app.session.flashbag.get('error') : false  %}
	{% if error %}
		"error": {{ error|json_encode|raw }},
	{% endif %}
  {% if revision_id is defined %}
		"revision_id": {{ revision_id|json_encode|raw }},
	{% endif %}
  {% if ouuid is defined %}
    "ouuid": {{ ouuid|json_encode|raw }},
  {% endif %}
  {% if uuid is defined %}
    "uuid": {{ uuid|json_encode|raw }},
  {% endif %}
  {% if type is defined %}
    "type": {{ type|json_encode|raw }},
  {% endif %}
	{% block body %}{% endblock %}
   "success": {{ success|json_encode|raw }}
}