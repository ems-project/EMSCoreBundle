{% extends '@EMSCore/base.html.twig' %}{% trans_default_domain 'EMSCoreBundle' %}

{% block title %}{{ 'form.contenttype.json_update.title'|trans({'%content_type%': contentType.singularName }) }}{% endblock %}
{% block pagetitle %}{{ block('title') }}{% endblock %}
{% block subtitle %}<small>{{ 'form.contenttype.json_update.subtitle'|trans({'%content_type%': contentType.singularName }) }}</small>{% endblock %}


{% block body %}
	<div class="row">
		<div class="col-md-4 col-md-offset-4">
			<div class="box box-primary">
				<div class="box-header with-border">
					<h3 class="box-title">{{ block('title') }}</h3>
				</div>
				<div class="box-body with-border">
					{{ form_start(form) }}
					{{ form_end(form) }}
				</div>
			</div>
		</div>
	</div>
{% endblock %}