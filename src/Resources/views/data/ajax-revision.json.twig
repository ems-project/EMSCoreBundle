{% extends '@EMSCore/ajax/notification.json.twig' %}{% trans_default_domain 'EMSCoreBundle' %}

{% block body %}
   {% if formErrors|length > 0 %}
		"formErrors": [
		{% for error in formErrors %}
			{
				"propertyPath": {{ error|emsco_property_path|json_encode|raw }},
				"message": {{ error.message|json_encode|raw }}
			}
			{% if not loop.last %},{% endif %}
		{% endfor %}
		] ,
	{% endif %}
{% endblock %}
